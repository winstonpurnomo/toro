{
  "name": "toro-router",
  "version": "0.0.2-alpha.0",
  "description": "A simple terminal router for React/Ink",
  "author": "Winston Purnomo",
  "license": "MIT",
  "keywords": [
    "react",
    "ink",
    "router",
    "terminal",
    "cli"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "react": "^19.1.0",
    "tsdown": "^0.13.0",
    "typescript": "5.8.3",
    "zod": "^4.0.10"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "zod": "^4.0.10"
  }
}
